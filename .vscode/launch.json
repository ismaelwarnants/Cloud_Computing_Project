{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Test",
            "request": "launch",
            "mainClass": "Test"
        },
        {
            "type": "java",
            "name": "Launch old_App",
            "request": "launch",
            "mainClass": "be.cloud.old_App",
            "projectName": ""
        },
        {
            "type": "java",
            "name": "Launch App",
            "request": "launch",
            "mainClass": "be.cloud.App",
            "projectName": ""
        },
        {
            "type": "java",
            "name": "Launch Main",
            "request": "launch",
            "mainClass": "main.java.Main",
            "projectName": ""
        },
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/serviceBreuk/bin/Debug/net6.0/serviceBreuk.dll",
            "args": [],
            "cwd": "${workspaceFolder}/serviceBreuk",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}